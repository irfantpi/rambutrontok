<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.SistemPakarRambutRontok" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.SistemPakarRambutRontok.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/theme</item>
    </style>



    <style name="Theme.SistemPakarRambutRontok.PengenalanPenyakitTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/c4</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/c4</item>
    </style>

    <style name="Theme.SistemPakarRambutRontok.DiagnosaTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/c6</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">#3a7bd5</item>
    </style>

    <style name="Theme.SistemPakarRambutRontok.SSTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">#5d86de</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">#5d86de</item>
    </style>

    <style name="Theme.SistemPakarRambutRontok.HistoryTheme">
        <item name="colorPrimaryDark">@color/c6</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">#3a7bd5</item>
        <item name="colorPrimary">#3a7bd5</item>
    </style>

    <style name="Theme.SistemPakarRambutRontok.HomeTheme">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimaryDark">@color/white</item>
        <item name="android:statusBarColor" tools:ignore="NewApi">@color/white</item>
    </style>

    <style name="Theme.SistemPakarRambutRontok.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.SistemPakarRambutRontok.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <color name="c1">#D7D9D2</color>
    <color name="c2">#130974</color>
    <color name="c3">#b1abbe</color>
    <color name="c4">#403eb0</color>
    <color name="c5">#fe5c1d</color>
    <color name="c6">#e83319</color>
    <color name="c7">#d79261</color>
    <color name="c8">#433a3d</color>
</resources>